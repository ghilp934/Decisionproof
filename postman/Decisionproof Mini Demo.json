						"packages": {},
						"requests": {},
						"exec": [
							"pm.test(\"health: status 200\", () => {\r",
							"  pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"health: response time < 1500ms\", () => {\r",
							"  pm.expect(pm.response.responseTime).to.be.below(1500);\r",
							"});\r",
							"\r",
							"// 응답이 JSON이든 텍스트든 'success' 표식이 있으면 통과\r",
							"pm.test(\"health: contains success marker\", () => {\r",
							"  const text = pm.response.text().toLowerCase();\r",
							"  let ok = text.includes(\"success\");\r",
							"\r",
							"  try {\r",
							"    const j = pm.response.json();\r",
							"    const flat = JSON.stringify(j).toLowerCase();\r",
							"    ok = ok || flat.includes(\"success\");\r",
							"  } catch (e) {}\r",
							"\r",
							"  pm.expect(ok, \"response should include 'success'\").to.be.true;\r",
							"});"
						]
					}
				}
